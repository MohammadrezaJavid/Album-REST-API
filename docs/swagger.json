                    "application/json"
                ],
                "tags": [
                    "CRUD Album"
                ],
                "summary": "Update one Album from database",
                "parameters": [
                    {
                        "description": "Album JSON",
                        "name": "album",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Album"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Album"
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Post an Album and saved to database",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "CRUD Album"
                ],
                "summary": "Post an Album",
